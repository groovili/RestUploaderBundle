{
  "name": "groovili/rest-uploader-bundle",
  "type": "symfony-bundle",
  "version": "1.0.0",
  "description": "A Symfony bundle to handle file upload and management for REST API",
  "keywords": [
    "symfony",
    "rest",
    "file",
    "upload",
    "validation"
  ],
  "homepage": "https://github.com/groovili/RestUploaderBundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Maxim Ivanov",
      "email": "m.ivanov.work@gmail.com",
      "homepage": "https://github.com/groovili"
    }
  ],
  "require": {
    "php": ">=7.1",
    "symfony/framework-bundle": "^4.1.12",
    "doctrine/doctrine-bundle": "^1.6",
    "doctrine/orm": "^2.5",
    "symfony/console": "^2.8|^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8|^5.0",
    "symfony/phpunit-bridge": "~3.2",
    "symfony/yaml": "^2.8|^3.0",
    "friendsofphp/php-cs-fixer": "^1.1"
  },
  "suggest": {
    "friendsofsymfony/rest-bundle": "API bundle to handle REST easier",
    "jms/serializer-bundle": "Serialaizer",
    "lexik/jwt-authentication-bundle": "Json web token for authentication",
    "nelmio/api-doc-bundle": "Automatic documentation generator"
  },
  "autoload": {
    "psr-4": {
      "Groovili\\RestUploaderBundle\\": ""
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    },
    "files": [
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1-dev"
    }
  }
}
